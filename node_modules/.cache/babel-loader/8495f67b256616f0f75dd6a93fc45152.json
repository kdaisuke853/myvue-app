sourcesContent":["<template>\n  <div class=\"container\">\n    <h2>ようこそ</h2>\n    <h2>{{ login_name }}さん</h2>\n\n    <button type=\"button\" @click=\"token_log()\">確認する</button>\n    <!--<input type =\"text\" v-model=\"this.info\">-->\n\n  </div>\n</template>\n\n<script>\n\nimport axios from '../axios-for-auth.js';\nexport default {\n  data: function () {\n  return {\n    info: ''\n  }\n},\n  computed: {\n    idToken() {\n      return this.$store.getters.idToken;\n    },\n    login_name: function(){\n      return this.info != null ? this.info :''\n    }\n  },\n  created(){\n      axios.get('/api/myself/',{\n          headers:{\n              Authorization: `Token ${this.idToken}`\n          }\n      })\n      .then((response) => {\n          this.info = response.data.username\n      })\n  },\n  methods: {\n    token_log() {\n      console.log(`${this.idToken}`)\n        axios.get('/api/myself/',{\n            headers: {\n                Authorization: `Token ${this.idToken}`\n            }\n        })\n        .then((response) => {\n          \n            console.log(response);       \n            //document.write('<p>'+ response.data + '</p>');\n            this.info = response.data.username\n        });\n    },\n  }\n}\n</script>\n"],"sourceRoot":"src/pages"}]}