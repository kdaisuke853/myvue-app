    <option value=\"Open\">始値(open)</option>\n            <option value=\"Low\">安値(low)</option>\n            <option value=\"High\">高値(high)</option>\n            <option value=\"Close\">終値(close)</option>\n        </select>\n\n       <button type=\"button\" @click=\"data_post(),show = !show\">実行</button>\n    </form>\n  </div>\n\n  <transition name = \"fade\"> \n    <div v-if=\"show\"> \n      <!--<button type=\"button\" @click=\"datareset(), show = !show\">リセット</button>-->\n      <h2>{{selected}}の予測が完了しました。</h2>\n      <h3>銘柄名:{{datas_parse.name}} 予測日時:{{resdata.date}} 値段:{{resdata.predict_target}}円</h3>\n    </div>\n  </transition>\n\n</div>\n</template>\n\n<script>\nimport axios from '../axios-for-auth.js';\nexport default {\n  data() {\n      return{\n      datas:localStorage.getItem('datalist'),\n      code:localStorage.getItem('code'),\n      datas_parse:JSON.parse(localStorage.getItem('datalist')),\n      resdata:\"\",\n      show:false,\n      selected: 'どの値を予測したいですか?',\n      }\n  },\n  methods: {\n    data_post() {\n        axios.post(\n            '/api/reserve_data/',\n        {\n            'Content-Type': 'application/json',\n            datalist:this.datas,\n            code:this.code,\n            target:this.selected,\n        }\n        ).then((response) => {\n          console.log(response);\n          this.resdata = response.data\n          localStorage.clear();\n        });\n      //axios.post(\n      //  '/auth/',\n      //  {\n      //    username: this.username,\n      //    password: this.password,\n      //    returnSecureToken: true\n      //  }\n      //).then((response) => {\n      //    this.$store.commit('updateIdToken', response.data.token); \n          //this.$router.push('/');\n      //  console.log(response);\n      //  console.log(this.$store)\n        //localStorage.setItem(\"my_token\", response.data.token);\n     //   this.$store.dispatch(\"auth\", {\n      //    userToken: response.data.token\n      //  });\n     // });\n    }\n  }\n};\n</script>\n\n<style>\n.input-group {\n  margin: 5px;\n}\n</style>"]}]}